    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="dc:description"/>
        <IRI>#Mode</IRI>
        <Literal datatypeIRI="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral">A Mode indicates the means by which the vehicle performs transportation (e.g. road, rail, etc).</Literal>
    </AnnotationAssertion>
