    <!-- http://ontology.eil.utoronto.ca/icity/iCity-MonetaryValue/1.0#MonetaryValue -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-MonetaryValue/1.0#MonetaryValue"/>
    


    <!-- http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#AccessMethod -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#AccessMethod">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#hasMonetaryCost"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-MonetaryValue/1.0#MonetaryValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#validFor"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.w3.org/2006/time#DurationDescription"/>
                            <rdf:Description rdf:about="http://www.wurvoc.org/vocabularies/om-1.8/Quantity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>An Access Method is the means of access to a Line
An AccessMethod has a Monetary Value.
An AccessMethod may be valid for a specific distance or time.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#Fare -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#Fare">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#AccessMethod"/>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#Pass -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#Pass">
