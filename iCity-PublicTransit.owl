    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#Route -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#Route">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-Change#manifestationOf"/>
                        <owl:someValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RoutePD"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-Change#manifestationOf"/>
                        <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RoutePD"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-Change#Manifestation"/>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-Change#existsAt"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-Mereology/1.0#contains"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteLink"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#hasSection"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteSection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route consists of a series of Route Sections.
A Route has some directionality.
A Route is executed by various Vehicles at different points in time.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteId -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteId">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#TransitOntologyThing"/>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteLink -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteLink">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#beginsAtStop"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#StopPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#endsAtStop"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#StopPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route Link begins and ends at a Stop Point.
A Route Link operates on an Arc.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RoutePD -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RoutePD">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-Change#hasManifestation"/>
                        <owl:someValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#Route"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-Change#hasManifestation"/>
                        <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#Route"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-Change#TimeVaryingConcept"/>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-Change#existsAt"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#Interval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#hasRouteId"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteId"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Routes change over time. RoutePD captures the entire entity of a route, i.e. the route perdurant.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteSection -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteSection">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-Mereology/1.0#contains"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteLink"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route Section is part of a Route and consists of Route Links.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteTimetable -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteTimetable">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#arrivalTime"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#DateTimeDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#timetableFor"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#Route"/>
                            <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#RouteLink"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:onClass>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#travelTime"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#DurationDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route Timetable represents schedule information for a particular Route, or Route Link.
A RouteTimetable has an expected travel time (Duration) for the Route, or Route Link.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#StopPoint -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#StopPoint">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/iCity-PublicTransit/1.1.owl#TransitOntologyThing"/>
