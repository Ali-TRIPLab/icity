            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icity/iCity-Change/1.1/manifestationOf"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/icity/iCity-TransportationSystem/1.1/ArcPD"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icity/iCity-Change/1.1/manifestationOf"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/icity/iCity-TransportationSystem/1.1/ArcPD"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/iCity-Change/1.1/Manifestation"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/iCity-TransportationSystem/1.1/TransportationOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/iCity-Mereology/1.0//1.0#componentOf"/>
