    </owl:Class>
    


    <!-- https://w3id.org/icity/MonetaryValue/Rate -->

    <owl:Class rdf:about="https://w3id.org/icity/MonetaryValue/Rate">
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/MonetaryValue/MonetaryValueOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/MonetaryValue/appliesFor"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#DurationDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Rate is a Monetary Value fee that applies for some Duration.</dc:description>
    </owl:Class>
    


