    <!-- https://w3id.org/icity/PublicTransit/AccessMethod -->

    <owl:Class rdf:about="https://w3id.org/icity/PublicTransit/AccessMethod">
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/PublicTransit/hasMonetaryCost"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/icity/OM/MonetaryValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/PublicTransit/validFor"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.w3.org/2006/time#DurationDescription"/>
                            <rdf:Description rdf:about="http://www.wurvoc.org/vocabularies/om-1.8/Quantity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>An Access Method is the means of access to a Line
An AccessMethod has a Monetary Value.
An AccessMethod may be valid for a specific distance or time.</dc:description>
    </owl:Class>
    


    <!-- https://w3id.org/icity/PublicTransit/Fare -->

    <owl:Class rdf:about="https://w3id.org/icity/PublicTransit/Fare">
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/PublicTransit/AccessMethod"/>
    </owl:Class>
    


    <!-- https://w3id.org/icity/PublicTransit/Pass -->

    <owl:Class rdf:about="https://w3id.org/icity/PublicTransit/Pass">
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/PublicTransit/AccessMethod"/>
    </owl:Class>
    


    <!-- https://w3id.org/icity/PublicTransit/Route -->

    <owl:Class rdf:about="https://w3id.org/icity/PublicTransit/Route">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icity/Change/manifestationOf"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/icity/PublicTransit/RoutePD"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icity/Change/manifestationOf"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/icity/PublicTransit/RoutePD"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/Change/Manifestation"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Change/existsAt"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Mereology/contains"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/icity/PublicTransit/RouteLink"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/PublicTransit/hasSection"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/icity/PublicTransit/RouteSection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route consists of a series of Route Sections.
A Route has some directionality.
A Route is executed by various Vehicles at different points in time.</dc:description>
    </owl:Class>
    


    <!-- https://w3id.org/icity/PublicTransit/RouteId -->

    <owl:Class rdf:about="https://w3id.org/icity/PublicTransit/RouteId">
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/PublicTransit/TransitOntologyThing"/>
    </owl:Class>
    


    <!-- https://w3id.org/icity/PublicTransit/RouteLink -->

    <owl:Class rdf:about="https://w3id.org/icity/PublicTransit/RouteLink">
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/PublicTransit/beginsAtStop"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/icity/PublicTransit/StopPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/PublicTransit/endsAtStop"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/icity/PublicTransit/StopPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route Link begins and ends at a Stop Point.
A Route Link operates on an Arc.</dc:description>
    </owl:Class>
    


    <!-- https://w3id.org/icity/PublicTransit/RoutePD -->

    <owl:Class rdf:about="https://w3id.org/icity/PublicTransit/RoutePD">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icity/Change/hasManifestation"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/icity/PublicTransit/Route"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icity/Change/hasManifestation"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/icity/PublicTransit/Route"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/Change/TimeVaryingConcept"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Change/existsAt"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#Interval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/PublicTransit/hasRouteId"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/icity/PublicTransit/RouteId"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Routes change over time. RoutePD captures the entire entity of a route, i.e. the route perdurant.</dc:description>
    </owl:Class>
    


    <!-- https://w3id.org/icity/PublicTransit/RouteSection -->

    <owl:Class rdf:about="https://w3id.org/icity/PublicTransit/RouteSection">
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Mereology/contains"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/icity/PublicTransit/RouteLink"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/PublicTransit/beginsAtStop"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/icity/PublicTransit/StopPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/PublicTransit/endsAtStop"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/icity/PublicTransit/StopPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route Section is part of a Route and consists of Route Links.</dc:description>
    </owl:Class>
    


    <!-- https://w3id.org/icity/PublicTransit/RouteTimetable -->

    <owl:Class rdf:about="https://w3id.org/icity/PublicTransit/RouteTimetable">
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/PublicTransit/arrivalTime"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#DateTimeDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/PublicTransit/timetableFor"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/icity/PublicTransit/Route"/>
                            <rdf:Description rdf:about="https://w3id.org/icity/PublicTransit/RouteLink"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:onClass>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/PublicTransit/travelTime"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#DurationDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route Timetable represents schedule information for a particular Route, or Route Link.
A RouteTimetable has an expected travel time (Duration) for the Route, or Route Link.</dc:description>
    </owl:Class>
    


    <!-- https://w3id.org/icity/PublicTransit/StopPoint -->

    <owl:Class rdf:about="https://w3id.org/icity/PublicTransit/StopPoint">
        <rdfs:subClassOf rdf:resource="http://www.pms.ifi.uni-muenchen.de/OTN#Node"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/PublicTransit/TransitOntologyThing"/>
