    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/AccessMethod -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/AccessMethod">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/hasMonetaryCost"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/OM/MonetaryValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/validFor"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/Length"/>
                            <rdf:Description rdf:about="http://www.w3.org/2006/time#DurationDescription"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>An Access Method is the means of access to a Line
An AccessMethod has a Monetary Value.
An AccessMethod may be valid for a specific distance or time.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/AccessibleStopPoint -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/AccessibleStopPoint">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/StopPoint"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/wheelchairBoarding"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/StopPoint"/>
    </owl:Class>
    


