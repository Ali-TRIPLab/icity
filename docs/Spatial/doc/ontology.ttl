                                   <http://purl.org/vocab/vann/preferredNamespaceUri> "https://w3id.org/icity/Spatial/1.0/"^^xsd:string ;
                                   rdfs:comment """Reuses the GeoVocab Spatial Ontology.
		Copyright @ 2016 Megan Katsumi, iCity Research Group"""^^xsd:string ;
                                   owl:versionInfo 1.0 ;
                                   <http://www.w3.org/ns/adms#relatedDocumentation> "Under development. Please see report on iCity Ontology v1."^^xsd:string .

