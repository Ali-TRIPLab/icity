    <!-- https://w3id.org/icity/LandUse/ParcelPD -->


    <owl:Class rdf:about="https://w3id.org/icity/LandUse/ParcelPD">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icity/Change/1.1/hasManifestation"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/icity/LBCSv2/Parcel"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icity/Change/1.1/hasManifestation"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/icity/LBCSv2/Parcel"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/Change/1.1/TimeVaryingConcept"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/LandUse/LandUseOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Change/1.1/existsAt"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#Interval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/SpatialLoc/1.0/hasLocation"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/icity/SpatialLoc/1.0/SpatialFeature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A parcel may change over time, ParcelPD captures the entire entity of a parcel, i.e. the parcel perdurant.</dc:description>
    </owl:Class>
    


