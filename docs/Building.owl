                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icity/Change/hasManifestation"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/icity/Building/Building"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/Building/BuildingOntologyThing"/>
