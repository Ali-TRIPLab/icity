                         [ rdf:type owl:Restriction ;
                           owl:onProperty <https://w3id.org/icity/Mereology/1.0//1.0#hasComponent> ;
                           owl:allValuesFrom [ rdf:type owl:Class ;
                                               owl:unionOf ( :Arc
                                                             :Node
