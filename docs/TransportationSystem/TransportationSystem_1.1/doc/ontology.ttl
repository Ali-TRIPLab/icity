###  https://w3id.org/icity/TransportationSystem/AccessRestriction
:AccessRestriction rdf:type owl:Class ;
                   rdfs:subClassOf :TransportationOntologyThing ;
                   rdfs:comment """Future extensions should capture this semantics in greater detail.
AccessRestriction may apply to certain Vehicle types (bus, cab, eco-friendly), Vehicle properties (number of passengers), or attributes of the traveler themselves (e.g. no bicycles on transit between certain times)."""^^xsd:string .


###  https://w3id.org/icity/TransportationSystem/Arc
:Arc rdf:type owl:Class ;
     owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                  owl:onProperty <https://w3id.org/icity/Change/1.1/manifestationOf> ;
                                                  owl:someValuesFrom :ArcPD
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty <https://w3id.org/icity/Change/1.1/manifestationOf> ;
                                                  owl:allValuesFrom :ArcPD
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
     rdfs:subClassOf <https://w3id.org/icity/Change/1.1/Manifestation> ,
                     :TransportationOntologyThing ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty <https://w3id.org/icity/Mereology/1.0//1.0#componentOf> ;
                       owl:allValuesFrom :Network
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :accessesComplex ;
                       owl:allValuesFrom :TransportationComplex
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasControl ;
                       owl:allValuesFrom :AccessRestriction
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasSpeedLimit ;
                       owl:allValuesFrom :SpeedLimit
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty <https://w3id.org/icity/Change/1.1/existsAt> ;
                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                       owl:onClass <http://www.w3.org/2006/time#TemporalEntity>
                     ] .


###  https://w3id.org/icity/TransportationSystem/ArcPD
:ArcPD rdf:type owl:Class ;
