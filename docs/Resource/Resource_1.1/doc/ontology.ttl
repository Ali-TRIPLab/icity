                                                                         owl:allValuesFrom <http://ontology.eil.utoronto.ca/icity/Activity/Activity>
                                                                       ] ,
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty <http://ontology.eil.utoronto.ca/icity/Activity/effectOf> ;
