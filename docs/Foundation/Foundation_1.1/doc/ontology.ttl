                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <https://w3id.org/icity/Activity/hasParticipant> ;
                                                             owl:allValuesFrom <https://w3id.org/icity/Change/Manifestation>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <https://w3id.org/icity/Activity/hasPrecondition> ;
                                                             owl:allValuesFrom <https://w3id.org/icity/Activity/State>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <https://w3id.org/icity/Activity/hasSubactivity> ;
                                                             owl:allValuesFrom <https://w3id.org/icity/Activity/Activity>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <https://w3id.org/icity/Activity/occursAt> ;
                                                             owl:allValuesFrom <http://www.w3.org/2006/time#Interval>
                                                           ] ,
