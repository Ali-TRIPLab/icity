    <!-- http://ontology.eil.utoronto.ca/icity/cdm/Trip/Tour -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/cdm/Trip/Tour">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/cdm/Trip/Trip"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/cdm/Trip/startLoc"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty>
                            <rdf:Description>
                                <owl:inverseOf rdf:resource="http://ontology.eil.utoronto.ca/icity/cdm/Trip/endLoc"/>
                            </rdf:Description>
                        </owl:onProperty>
                        <owl:hasSelf rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasSelf>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/cdm/Activity/hasSubactivity"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/icity/cdm/Trip/Trip"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A sequence of Trips made by one Person.
A Tour starts and ends at the same Location.</dc:description>
        <rdfs:comment>Future work: define a new property to enforce the same start and end location constraint.</rdfs:comment>
    </owl:Class>



