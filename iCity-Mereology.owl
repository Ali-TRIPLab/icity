    </owl:ObjectProperty>
    


    <!-- https://w3id.org/icity/iCity-Mereology/1.0/hasProperPart -->

    <owl:ObjectProperty rdf:about="https://w3id.org/icity/iCity-Mereology/1.0/hasProperPart">
